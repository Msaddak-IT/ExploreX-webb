<center>
<div class="body-container">
<div class="form-container">
        {{ form_start(form, {'attr': {'novalidate': 'novalidate'}})}}

        {# Afficher les messages d'erreur globaux du formulaire #}
        {{ form_errors(form) }}
          <div>
            {{ form_label(form.destination) }}
            {{ form_widget(form.destination) }}
                <div style="color: red; font-size: 14px; margin-top: 5px;">
            {{ form_errors(form.destination) }}
           
        </div>
        <div>
            {{ form_label(form.debut) }}
            {{ form_widget(form.debut) }}
                <div style="color: red; font-size: 14px; margin-top: 5px;">
            {{ form_errors(form.debut) }}
           
        </div>
         <div>
            {{ form_label(form.fin) }}
            {{ form_widget(form.fin) }}
                <div style="color: red; font-size: 14px; margin-top: 5px;">
            {{ form_errors(form.fin) }}
           
        </div>
         <div>
            {{ form_label(form.prix) }}
            {{ form_widget(form.prix) }}
                <div style="color: red; font-size: 14px; margin-top: 5px;">
            {{ form_errors(form.prix) }}
           
        </div>
        <div>
            {{ form_label(form.service) }}
            {{ form_widget(form.service) }}
                <div style="color: red; font-size: 14px; margin-top: 5px;">
            {{ form_errors(form.service) }}
           
        </div>

         </table>

    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
</center>