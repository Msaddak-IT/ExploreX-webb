{% extends 'basenew.html.twig' %}

{% block title %}Bonplan{% endblock %}

{% block body %}
    <h1>Bonplan</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ bonplan.id }}</td>
            </tr>
            <tr>
                <th>NameBonPlan</th>
                <td>{{ bonplan.nameBonPlan }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ bonplan.rating }}</td>
            </tr>
            <tr>
                <th>StartDate</th>
                <td>{{ bonplan.startDate ? bonplan.startDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ bonplan.endDate ? bonplan.endDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>AvgPrice</th>
                <td>{{ bonplan.avgPrice }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ bonplan.location }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_bonplan_index') }}">back to list</a>

    <a href="{{ path('app_bonplan_edit', {'id': bonplan.id}) }}">edit</a>
    <a href="{{ path('app_bonplan_delete',{'id':bonplan.id}) }}">delete</a>

    {{ include('bonplan/_delete_form.html.twig') }}
{% endblock %}
