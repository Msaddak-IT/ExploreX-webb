{% extends 'baseback.html.twig' %}

{% block title %}Reservations index{% endblock %}

        
    

       

{% block main %}
    <h1>Service index</h1>

    <table class="table">
        <thead>
            <tr>
              
                <th>NomService</th>
                <th>DescriptionService</th>
                <th>PrixService</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for service in services %}
            <tr>
                
                <td>{{ service.nomService }}</td>
                <td>{{ service.descriptionService }}</td>
                <td>{{ service.prixService }}</td>
                <td>
                    <a href="{{ path('app_serviceajouterser_show', {'idService': service.idService}) }}">show</a>
                    <a href="{{ path('app_serviceajouterser_edit', {'idService': service.idService}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_serviceajouterser_new') }}">Create new</a>
{% endblock %}
